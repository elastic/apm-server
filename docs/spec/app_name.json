{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "doc/spec/app_name.json",
    "title": "App name property",
    "type": "object",
    "properties": {
        "name": {
            "description": "Immutable name of the app emitting this event",
            "type": "string",
            "pattern": "^[a-zA-Z0-9 _-]+$",
            "maxLength": 1024
        }
    },
    "required": ["name"]
}
