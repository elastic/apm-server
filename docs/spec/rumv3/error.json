            "null",
            "object"
          ],
          "additionalProperties": false,
          "patternProperties": {
            "^[^.*\"]*$": {
              "type": [
                "null",
                "string",
                "boolean",
                "number"
              ],
              "maxLength": 1024
            }
          }
        },
        "p": {
